    "keywords": ["hola", "api", "sdk"],
    "homepage": "https://github.com/Fr3on/hola-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Pharaoh",
            "email": "hello@fr3on.info"
        }
    ],
    "require": {
      "php" : ">=5.6.0",
      "ext-curl" : "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Hola\\SDK\\" : "src"
        }
    }
}
